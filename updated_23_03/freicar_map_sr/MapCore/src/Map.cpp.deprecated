        }
        if ((heading_to_out_2 > (JUNCTION_STRAIGHT_ANGLE - JUNCTION_STRAIGHT_MARGIN)) &&
            (heading_to_out_2 < (JUNCTION_STRAIGHT_ANGLE + JUNCTION_STRAIGHT_MARGIN))) {
            // drive straight is valid for outgoing lane 2
